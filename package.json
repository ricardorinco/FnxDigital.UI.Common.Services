{
    "name": "fnxdigital-ui-common-services",
    "description": "FnxDigital - UI: Common Services",
    "version": "0.4.0",
    "scripts": {
        "build": "ng build fnx-services",
        "build:prod": "ng build fnx-services --prod  && gulp replaceVersion && cd dist/fnx-services && npm pack",
        "docs": "compodoc -p tsconfig.json --output dist/docs --includes projects/fnx-services --serve --watch --open",
        "ng": "ng",
        "prepare": "husky install",
        "release": "standard-version",
        "release:minor": "standard-version --release-as minor",
        "release:patch": "standard-version --release-as patch",
        "release:major": "standard-version --release-as major",
        "sonar": "sonar-scanner",
        "test": "ng test fnx-services --code-coverage",
        "test:pre-commit": "ng test fnx-services --browsers=ChromeHeadless --no-watch"
    },
    "private": true,
    "dependencies": {
        "@angular/animations": "~11.2.5",
        "@angular/common": "~11.2.5",
        "@angular/compiler": "~11.2.5",
        "@angular/core": "~11.2.5",
        "@angular/forms": "~11.2.5",
        "@angular/platform-browser": "~11.2.5",
        "@angular/platform-browser-dynamic": "~11.2.5",
        "@angular/router": "~11.2.5",
        "crypto-js": "^4.0.0",
        "gulp-clean": "^0.4.0",
        "gulp-run": "^1.7.1",
        "rxjs": "~6.6.0",
        "tslib": "^2.0.0",
        "zone.js": "~0.11.3"
    },
    "devDependencies": {
        "@angular-devkit/build-angular": "~0.1102.5",
        "@angular/cli": "~11.2.4",
        "@angular/compiler-cli": "~11.2.5",
        "@commitlint/cli": "^12.0.1",
        "@commitlint/config-angular": "^12.0.1",
        "@compodoc/compodoc": "^1.1.11",
        "@types/crypto-js": "^3.1.44",
        "@types/faker": "^5.1.5",
        "@types/jasmine": "~3.6.0",
        "@types/node": "^12.11.1",
        "codelyzer": "^6.0.0",
        "faker": "^5.1.0",
        "gulp": "^4.0.2",
        "gulp-filter": "^6.0.0",
        "gulp-foreach": "^0.1.0",
        "gulp-git": "^2.10.0",
        "gulp-sync": "^0.1.4",
        "gulp-tap": "^2.0.0",
        "gulp-util": "^3.0.8",
        "husky": "^6.0.0",
        "jasmine-core": "~3.6.0",
        "jasmine-spec-reporter": "~5.0.0",
        "karma": "~6.1.0",
        "karma-chrome-launcher": "~3.1.0",
        "karma-coverage": "~2.0.3",
        "karma-coverage-istanbul-reporter": "~3.0.3",
        "karma-jasmine": "~4.0.0",
        "karma-jasmine-html-reporter": "^1.5.0",
        "karma-junit-reporter": "^2.0.1",
        "karma-sonarqube-unit-reporter": "0.0.23",
        "ng-packagr": "^11.0.0",
        "protractor": "~7.0.0",
        "sonar-scanner": "^3.1.0",
        "standard-version": "^9.1.1",
        "ts-node": "~8.3.0",
        "tslint": "~6.1.0",
        "typescript": "~4.1.5"
    }
}
